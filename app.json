{
  "name": "airport",
  "description": "deploy VV to Heroku.",
  "keywords": [
    "v2ray"
  ],
  "env": {
    "PROTOCOL": {
      "description": "协议vmess或vless",
      "value": "vmess",
      "required": true
    },
    "UUID": {
      "description": "用户的主ID, uuidgenerator.net",
      "value": "2d728d8d-38cd-4265-9c6f-d89727f7e708",
      "required": false
    },
    "WS_PATH": {
      "description": "websocket路径",
      "value": "/ray",
      "required": true
    }
  },
  "website": "https://github.com/libsgh/v2ray-heroku",
  "repository": "https://github.com/libsgh/v2ray-heroku",
  "stack": "container",
  "features": [
    "runtime-dyno-metadata"
  ]
}
